SELECT Saccharomyces_cerevisiae_core_57_1j.gene_stable_id.stable_id, Saccharomyces_cerevisiae_core_57_1j.gene.seq_region_id, Saccharomyces_cerevisiae_core_57_1j.gene.seq_region_start, Saccharomyces_cerevisiae_core_57_1j.gene.seq_region_end, Saccharomyces_cerevisiae_core_57_1j.gene.seq_region_strand, Saccharomyces_cerevisiae_core_57_1j.gene.description 
FROM Saccharomyces_cerevisiae_core_57_1j.gene, Saccharomyces_cerevisiae_core_57_1j.gene_stable_id
WHERE Saccharomyces_cerevisiae_core_57_1j.gene.gene_id = Saccharomyces_cerevisiae_core_57_1j.gene_stable_id.gene_id
AND Saccharomyces_cerevisiae_core_57_1j.gene.seq_region_id = 3;